{
  "name": "simon-ehrlich-wager",
  "version": "1.0.0",
  "description": "A visualization of the long term, inflation-adjusted prices of the five metals used in the Simon-Ehrlich wager",
  "main": "index.js",
  "author": "W. Evan Sheehan <wallace.sheehan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dist": "mkdir -p dist/ && cp -rv static/* dist/ && npm test && rollup -c",
    "clean": "rm -rf build/ dist/*",
    "prepare": "./scripts/pivot.sh ./data/metals.tsv ./data/metals.json",
    "start": "rollup -w -c rollup.dev.config.js",
    "test": "BABEL_ENV=test tape -r babel-register test/**/*.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-svelte": "^3.1.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "d3-collection": "^1.0.4",
    "d3-scale": "^1.0.6"
  }
}
